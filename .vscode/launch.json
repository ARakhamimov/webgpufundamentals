  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Test ld-check-dependencies",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "node_modules/.bin/ld-check-dependencies"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Debug Build",
      "program": "${workspaceFolder}/node_modules/grunt/bin/grunt",
      "env": {
        "ARTICLE_FILTER_XXX": "webgl"
      },
      "args": [
        "buildlessons"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Diagram Gen",
      "runtimeArgs": [
          "--loader",
          "ts-node/esm"
      ],
      "program": "${workspaceFolder}/build/make-type-diagram.ts",
      "env": {
      },
      "args": [
      ]
    }
  ]
}